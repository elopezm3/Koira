Businesses
<% if @businesses %>
  <% @businesses.each do |business| %>
    <%= business.name %>
  <% end %>
<% end %>
Products or Services
<% if @product_or_services %>
  <% @product_or_services.each do |product_service| %>
    <%= link to business_path(product_service.business), product_service.name %>
  <% end %>
<% end %>
Categories
<% if @categories %>
  <% @categories.each do |category| %>
    <%= category.name %>
  <% end %>
<% end %>
