          <div class="form-inputs">
            <%= f.input :email,
                        required: false,
                        autofocus: true,
                        input_html: { autocomplete: "email" },
                        placeholder: "info@koira.com" %>
            <%= f.input :password,
                        required: false,
                        input_html: { autocomplete: "current-password" },
                        placeholder: "***************" %>
            <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
          </div>
